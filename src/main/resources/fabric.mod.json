{
	"schemaVersion": 1,
	"id": "suboptiomal-blocks",
	"version": "${version}",
	"name": "Suboptiomal Blocks",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/suboptiomal-blocks/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.shirojr.suboptiomal.blocks.SuboptimalBlocks",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "net.shirojr.suboptiomal.blocks.SuboptimalBlocksClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.shirojr.suboptiomal.blocks.SuboptimalBlocksDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"suboptiomal-blocks.mixins.json",
		{
			"config": "suboptiomal-blocks.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.19.4",
		"java": ">=17",
		"fabric-api": "*",
		"fabric-language-kotlin": ">=1.8.21"
	},
	"suggests": {
		"another-mod": "*"
	}
}